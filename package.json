{
  "name": "@adgorithmics/cinnamon-javascript-sdk",
  "version": "0.0.6-rc.5",
  "description": "",
  "main": "index.node.js",
  "browser": "index.web.js",
  "types": "src/index.d.ts",
  "files": [
    "/src/**/*.d.ts",
    "/index.web.js",
    "/index.node.js"
  ],
  "scripts": {
    "build": "npm run build-types && npm run build-ts",
    "build-ts": "webpack",
    "build-types": "SCHEMA_PATH='http://localhost:4001/v1/graphql' graphql-codegen --config codegen.yml",
    "lint": "concurrently \"npm run lint:eslint\" \"npm run lint:prettier\"",
    "lint:eslint": "eslint . --ext ts",
    "lint:eslint:fix": "eslint . --ext ts --fix",
    "lint:prettier": "prettier \"./src/**/*.ts\" -l --ignore-path .eslintignore",
    "lint:prettier:fix": "prettier \"./src/**/*.ts\" --write --ignore-path ./.eslintignore"
  },
  "author": "Adgorithmics, Inc.",
  "license": "MIT",
  "dependencies": {
    "@adgorithmics/graphql-errors": "^1.1.0",
    "cross-fetch": "3.0.4",
    "lodash.get": "4.4.2",
    "md5": "2.2.1"
  },
  "devDependencies": {
    "@adgorithmics/eslint-config-base": "1.3.1",
    "@graphql-codegen/cli": "1.8.3",
    "@graphql-codegen/introspection": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-resolvers": "1.8.3",
    "@types/graphql": "14.5.0",
    "@types/lodash.get": "4.4.6",
    "@types/md5": "2.1.33",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "acorn": "7.1.0",
    "concurrently": "5.0.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "graphql": "14.5.8",
    "prettier": "1.19.1",
    "ts-loader": "6.2.1",
    "ts-transformer-keys": "0.4.0",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 4,
    "singleQuote": true
  }
}
