{
  "name": "@adgorithmics/cinnamon-javascript-sdk",
  "version": "0.0.5-rc.3",
  "description": "",
  "main": "index.node.js",
  "browser": "index.web.js",
  "types": "src/index.d.ts",
  "files": [
    "/src",
    "/index.web.js",
    "/index.node.js"
  ],
  "scripts": {
    "build": "npm run build-types && npm run build-ts",
    "build-ts": "webpack",
    "build-types": "SCHEMA_PATH='http://localhost:4001/v1/graphql' graphql-codegen --config codegen.yml",
    "lint": "concurrently \"npm run lint:eslint\" \"npm run lint:prettier\"",
    "lint:eslint": "eslint . --ext ts",
    "lint:eslint:fix": "eslint . --ext ts --fix",
    "lint:prettier": "prettier \"./src/**/*.ts\" -l --ignore-path .eslintignore",
    "lint:prettier:fix": "prettier \"./src/**/*.ts\" --write --ignore-path ./.eslintignore"
  },
  "author": "Adgorithmics, Inc.",
  "license": "MIT",
  "dependencies": {
    "@adgorithmics/graphql-errors": "^1.0.3",
    "cross-fetch": "3.0.4",
    "lodash.get": "4.4.2"
  },
  "devDependencies": {
    "@adgorithmics/eslint-config-base": "1.3.0",
    "@graphql-codegen/cli": "1.6.1",
    "@graphql-codegen/introspection": "1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-resolvers": "1.6.1",
    "@types/graphql": "14.2.3",
    "@types/lodash.get": "4.4.6",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "acorn": "^6.3.0",
    "concurrently": "4.1.2",
    "eslint": "6.2.0",
    "eslint-config-prettier": "6.1.0",
    "graphql": "14.4.2",
    "prettier": "1.18.2",
    "ts-loader": "6.0.4",
    "ts-transformer-keys": "0.3.5",
    "typescript": "3.5.3",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 4,
    "singleQuote": true
  }
}
